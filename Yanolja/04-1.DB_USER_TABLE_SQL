-- 회원 테이블
CREATE TABLE USERSS (
    U_ID VARCHAR2(50) not null, -- 회원 ID
    U_EMAIL1 VARCHAR2(50) not null, -- 회원 이메일1
    U_EMAIL2 VARCHAR2(50) not null, -- 회원 이메일2
    U_PW VARCHAR2(50) not null, -- 회원 비밀번호
    U_NAME VARCHAR2(50) not null, -- 회원 이름
    U_PHONE1 VARCHAR2(50) not null, -- 회원 전화번호1
    U_PHONE2 VARCHAR2(50) not null -- 회원 전화번호2
);